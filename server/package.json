{
  "name": "server",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "setup:registration-tables": "node database/setupRegistrationTables.js",
    "migrate": "node database/migrations/migrate.js migrate",
    "migrate:status": "node database/migrations/migrate.js status",
    "fix:passwords": "node scripts/fix-registrar-passwords.js"
  },
  "vitest": {
    "coverage": {
      "provider": "v8",
      "reporter": [
        "text",
        "html"
      ]
    }
  },
  "dependencies": {
    "axios": "^1.7.9",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "google-auth-library": "^10.5.0",
    "morgan": "^1.10.1",
    "p-limit": "^5.0.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "puppeteer": "^24.29.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "nodemon": "^3.1.0",
    "supertest": "^7.1.4",
    "vitest": "^3.2.4"
  }
}
